extends layout

block content
  h1= title
  h1 Mahendra kumar Veeravalli

 
  - 
    var results = [
      { instrument_name: 'Guitar', type: 'String', year_made: 1995 },
      { instrument_name: 'Trumpet', type: 'Brass', year_made: 1980 },
      { instrument_name: 'Piano', type: 'Percussion', year_made: 2000 }
    ];
 
  .container-fluid
    .row
      each instrument in results
        .col-sm-2
          h3 #{instrument.instrument_name}
          p Type: #{instrument.type}
          p Year Made: #{instrument.year_made}