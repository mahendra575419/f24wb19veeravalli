extends layout
 
block content
    h1= title
    - var image_names = ['image1.jpeg', 'image2.jpeg', 'image3.jpeg', 'image4.jpeg', 'image5.jpeg'];
    - for (let i = 0; i < 3; i++)
        - var randomIndex = Math.floor(Math.random() * 5);
        img(src=`/images/${image_names[randomIndex]}`, alt=`Image ${i + 1}`, height='300', width='300')
